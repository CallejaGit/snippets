snippet html5
	<!DOCTYPE html>
	<html>
	    <head>
	    	<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<title>${1:`substitute(vim_snippets#Filename('', 'Page Title'), '^.', '\u&', '')`}</title>${2:link}
	    </head>
	    <body>
			${0:body}
	    </body>
	</html>

snippet div
	<div> ${1: body} </div>

snippet p
	<p> ${1:body} </p>

snippet h1
	<h1> ${1:body} </h1>

snippet li
	<li>
	
	</li>

snippet hr
	<a href="${1:LINK}">
	
